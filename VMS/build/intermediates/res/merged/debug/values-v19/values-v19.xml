<?xml version="1.0" encoding="utf-8"?>
<resources>
    <bool name="use_activity">false</bool>
    <bool name="use_provider">true</bool>
    <dimen name="statusMargin">25dp</dimen>
    <style name="GoogleNavigationDrawerTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowTranslucentStatus">true</item>
    </style>
    <style name="GoogleNavigationDrawerTheme.Light" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowTranslucentStatus">true</item>
    </style>
    <style name="GoogleNavigationDrawerTheme.Light.DarkActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="theme">@style/ThemeOverlay.AppCompat.Dark.ActionBar</item>
        <item name="popupTheme">@style/Base.V21.Theme.AppCompat.Light.Dialog</item>
    </style>
</resources>